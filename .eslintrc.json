{
    "extends": [
        "eslint:recommended",
        "plugin:prettier/recommended"
    ],
    "env": { "node": true, "es6": true },
    "rules": {
        "no-empty": "off",
        "no-unused-vars": "off",
        "no-explicit-any": "off",
        "prettier/prettier": [
            "error",
            {
                "singleQuote": true,
                "semi": true,
                "printWidth": 100
            }
        ]
    }
}