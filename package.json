{
  "name": "browserify-es6-sass-env",
  "description": "A light frontend development tool that uses no task runners.",
  "authors": "stoiel",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/stoiel/browserify-es6-sass-env.git"
  },
  "license": "GPL-3.0",
  "scripts": {
    "postinstall": "bash scripts/postinstall.sh",
    "build:sass": "env-cmd -f .env bash scripts/build_style.sh",
    "build:js": "env-cmd -f .env node src/bundleApp.js",
    "build": "parallelshell \"npm run build:sass\" \"npm run build:js\"",
    "watch:sass": "env-cmd -f .env bash scripts/watch_style.sh",
    "watch:js": "env-cmd -f .env bash scripts/watch_js.sh",
    "watch": "env-cmd -f .env bash scripts/watch.sh"
  },
  "alias": {},
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "autoprefixer": "^9.5.1",
    "babel-jest": "^24.8.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-preset-minify": "^0.5.0",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "browserslist": "^4.6.1",
    "core-js": "^3.1.3",
    "env-cmd": "^9.0.1",
    "glob": "^7.1.4",
    "jest": "^24.8.0",
    "jquery": "^3.4.1",
    "node-sass": "^4.12.0",
    "nodemon": "^1.19.1",
    "npm-watch": "^0.6.0",
    "parallelshell": "3.0.2",
    "path": "^0.12.7",
    "postcss-cli": "^6.1.2",
    "recursive-readdir": "^2.2.2"
  }
}
